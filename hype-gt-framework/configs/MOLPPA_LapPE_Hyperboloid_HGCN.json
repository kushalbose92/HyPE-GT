{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "GraphTransformer",
    "dataset": "ogbg-ppa",
    
    "out_dir": "out/MOLPPA_sparse_LapPE_LN/",
    
    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 32,
        "init_lr": 0.0007,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 15,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 24
    },
    
    "net_params": {
        "L": 3,
        "n_heads": 4,
        "hidden_dim": 64,
        "out_dim": 64,
        "edge_feat": true,
        "residual": true,
        "readout": "mean",
        "c": 1.0,
        "act": "relu",
        "manifold": "Hyperboloid",
        "model": "HGCN",
        "pe_layers": 2,
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "layer_norm": true,
        "batch_norm": false,
        "self_loop": false,
        "pos_enc_dim": 8,
        "lap_pos_enc": true,
        "wl_pos_enc": false,
        "rw_pos_enc": false,
        "full_graph": false
    }
}


